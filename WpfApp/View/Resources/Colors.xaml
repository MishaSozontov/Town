<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--#region Базовые цвета-->
    <SolidColorBrush x:Key="Base.White" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="Base.Dark" Color="#12877F"/>
    <SolidColorBrush x:Key="Base.Light" Color="#47DCD1"/>
    <SolidColorBrush x:Key="Base.PaleLight" Color="#FFD9FDFF"/>

    <!--#endregion-->
    <!--#region Дополнительные цвета-->
    <SolidColorBrush x:Key="Additional.Dark" Color="#CF8700"/>
    <SolidColorBrush x:Key="Additional.Light" Color="#FFC352"/>
</ResourceDictionary>